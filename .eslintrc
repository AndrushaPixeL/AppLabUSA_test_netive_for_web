{
    "env": {
        "browser": true,
        "es2021": true 
    },
    "extends": [ 
        "eslint:recommended",
        "plugin:react/recommended",
        "prettier"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react", 
        "prettier"
    ],
    "rules": {
    "prettier/prettier": ["warn", {
        "endOfLine": "auto"
      }],
      "react/jsx-fragments": "off",
      "import/prefer-default-export": "off",
      "@typescript-eslint/no-unused-vars": "warn",
      "no-console": "off",
      "react/prop-types": 0
    }
}